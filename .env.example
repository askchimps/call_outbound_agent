LIVEKIT_URL=<your LiveKit server URL>
LIVEKIT_API_KEY=<your API Key>
LIVEKIT_API_SECRET=<your API Secret>
DEEPGRAM_API_KEY=<your Deepgram API Key>
OPENAI_API_KEY=<your OpenAI API Key>
SIP_OUTBOUND_TRUNK_ID=<your SIP outbound trunk ID>

# TTS Configuration - choose "elevenlabs" or "cartesia"
TTS_PROVIDER=elevenlabs

# ElevenLabs TTS settings (when TTS_PROVIDER=elevenlabs)
ELEVEN_API_KEY=<your ElevenLabs API Key>
TTS_VOICE_ID=21m00Tcm4TlvDq8ikWAM
TTS_MODEL_ID=eleven_flash_v2_5

# Cartesia TTS settings (when TTS_PROVIDER=cartesia)
CARTESIA_API_KEY=<your Cartesia API Key>
CARTESIA_VOICE_ID=a0e99841-438c-4a64-b679-ae501e7d6091
CARTESIA_MODEL=sonic-2

# LLM Configuration
# Default: Uses OpenAI with OPENAI_API_KEY above
# LLM_MODEL=gpt-4o-mini

# For self-hosted vLLM with xLAM (recommended for function calling):
# xLAM-7b-fc-r: 88.24% BFCL score, #3 globally, best accuracy/speed balance
# xLAM-1b-fc-r: 78.94% BFCL score, ultra-fast, good for low latency
#
# vLLM command: vllm serve Salesforce/xLAM-7b-fc-r --enable-auto-tool-choice --tool-call-parser xlam
#
# LLM_MODEL=Salesforce/xLAM-7b-fc-r
# LLM_BASE_URL=http://your-vllm-server:8000/v1
# OPENAI_API_KEY=not-needed